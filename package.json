{
  "name": "objectsm",
  "version": "2.0.2",
  "description": "JS Object Serializer/Deserializer Manager to convert JS objects with saved metadata back into instances of the class they were created with. This library intends to behave like PHP serialize/deserialize does.",
  "exports": "./dist/index.js",
  "scripts": {
    "test": "flow && jest",
    "build": "gulp build",
    "validate": "gulp validate",
    "line": "gulp validate",
    "node": "cross-env NODE_ENV=development build/devnode.js",
    "prepack": "npm run build",
    "start": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aikar/objectsm.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "json",
    "js",
    "object",
    "php",
    "serializer",
    "deserializer",
    "mapper"
  ],
  "author": "Daniel Ennis <aikar@aikar.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aikar/objectsm/issues"
  },
  "homepage": "https://github.com/aikar/objectsm#readme",
  "engines": {
    "node": ">= 8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-syntax-flow": "^7.21.4",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/register": "^7.21.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chalk": "^2.1.0",
    "cross-env": "^5.1.0",
    "eslint": "^8.40.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-flowtype-errors": "3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "flow-bin": "0.85.0",
    "flow-typed": "^2.2.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-changed-in-place": "^2.3.0",
    "gulp-concat": "^2.6.1",
