    "postpack": "build/postpack.sh",
    "start": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aikar/objectsm.git"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "json",
    "js",
    "object",
    "php",
    "serializer",
    "deserializer",
    "mapper"
